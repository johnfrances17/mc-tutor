{
  "version": 2,
  "builds": [
    {
      "src": "server/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/src/server.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/html/index.html"
    }
  ],
  "public": true
}
