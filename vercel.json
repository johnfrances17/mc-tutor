{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.7.3"
    },
    {
      "src": "main/**/*.php",
      "use": "vercel-php@0.7.3"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.php",
      "dest": "/$1.php"
    },
    {
      "src": "/main/(.*)",
      "dest": "/main/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ],

  "regions": ["sin1"]
}
